{
  "name": "ppu-compare",
  "version": "0.2.0",
  "private": true,
  "homepage": "https://taylordevereaux.github.io/ppu-compare/",
  "dependencies": {
    "accounting-js": "^1.1.1",
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "convert-js": "^1.0.5",
    "eslint": "^4.16.0",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.13.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.0",
    "reactstrap": "^5.0.0-alpha.4",
    "tether": "^1.4.3",
    "webpack": "^3.10.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom"
  }
}
